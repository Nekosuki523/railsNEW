<h3>予定詳細</h3>

<table>
  <tr>
  <th>タイトル</th>
  <td><%= @todo.title %></td>
  </tr>

  <tr>
  <th>開始日</th>
  <td><%= @todo.start_date %></td>
  </tr>

  <tr>
  <th>終了日</th>
  <td><%= @todo.end_date %></td>
  </tr>
    
  <tr>
  <th>終日</th>
  <th>
  <% if @todo.allday == "true" %>
  ○
  <% else @todo.allday == "false" %>
        
  <% end %>
  </th>
  </tr>

  <tr>
  <th>スケジュールメモ</th>
  <td><%= @todo.memo %></td>
  </tr>
</table>

  <div>
  <ul>
  <li><%= link_to "予定一覧に戻る", :todos %></li>
  </ul>
  </div>
 
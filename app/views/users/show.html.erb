<h3>予定詳細</h3>


  <table>
    <tr>
      <th>タイトル</th>
      <td><%= @user.title %></td>
    </tr>

    <tr>
      <th>開始日</th>
      <td><%= @user.start_date %></td>
    </tr>

    <tr>
      <th>終了日</th>
      <td><%= @user.end_date %></td>
    </tr>
    
    <tr>
        <th>終日</th>
        <th>
        <% if @user.allday == "true" %>
        ○
        <% else @user.allday == "false" %>
        
        <% end %>
        </th>
    </tr>
<!--終日のチェックボックス-->
    <tr>
      <th>スケジュールメモ</th>
      <td><%= @user.memo %></td>
    </tr>
  </table>

  <div>
    <ul>
      <li><%= link_to "予定一覧に戻る", :users %></li>
    </ul>
  </div>
 